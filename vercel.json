{
  "version": 2,
  "builds": [
    { "src": "gmailtracker.py", "use": "@vercel/python" }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "emailtracker.py"
    },
    {
      "src": "/pixel.png",
      "dest": "emailtracker.py"
    },
    {
      "src": "/send-email/(.*)",
      "dest": "emailtracker.py"
    }
  ]
}
